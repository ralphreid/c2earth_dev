    #error_explanation
      %h2= "#{pluralize(@stakeholder_company_type.errors.count, "error")} prohibited this stakeholder_company_type from being saved:"
      %ul
        - @stakeholder_company_type.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :takeholder_company_type
    = f.text_field :takeholder_company_type
  .actions
    = f.submit 'Save'
